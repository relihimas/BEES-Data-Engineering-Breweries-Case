{
  "id": null,
  "uid": "airflow-overview",
  "title": "Airflow - Overview",
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Falhas de tasks (última 1h)",
      "datasource": "Prometheus",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(airflow_task_failure_total[1h]))"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "barchart",
      "title": "Falhas por DAG (últimas 6h)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (dag_id) (increase(airflow_task_failure_total[6h]))"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "logs",
      "title": "Erros recentes nas DAGs (logs Airflow)",
      "datasource": "Loki",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 12 },
      "targets": [
        {
          "refId": "A",
          "expr": "{container=~\"bees-rachid-airflow-.*\"} |= \"ERROR\""
        }
      ],
      "options": {
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true
      }
    }
  ],
  "templating": {
    "list": []
  }
}